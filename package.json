{
  "name": "mst-tools",
  "version": "3.1.1",
  "description": "Toolset for efficient development with MobX-State-Tree",
  "license": "MIT",
  "homepage": "https://github.com/keindev/mst-tools#readme",
  "author": {
    "name": "Daniil Ryazanov",
    "email": "kein@tagproject.ru"
  },
  "keywords": [
    "mobx",
    "mobx-state-tree",
    "tools",
    "toolset"
  ],
  "bugs": "https://github.com/keindev/mst-tools/issues",
  "types": "./lib/index.d.ts",
  "type": "commonjs",
  "main": "lib/mst-tools.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keindev/mst-tools.git"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:ts": "rimraf lib tmp && tsc --extendedDiagnostics --importHelpers && rollup -c && rimraf tmp",
    "generate": "npm-run-all generate:*",
    "generate:changelog": "node --experimental-specifier-resolution=node node_modules/.bin/changelog generate --bump",
    "generate:ghinfo": "node --experimental-specifier-resolution=node node_modules/.bin/ghinfo generate -d media -t utils",
    "generate:media": "node --experimental-specifier-resolution=node node_modules/.bin/figma-portal extract -d media -p GitHub",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "rimraf coverage && eslint src --ext .ts",
    "lint:spell": "cspell -c .vscode/cspell.json --no-summary \"**/*.{js,ts,tsx,md,yml,json}\"",
    "prepare": "npm-run-all prepare:*",
    "prepare:config": "node --experimental-specifier-resolution=node node_modules/.bin/ts-library-shared-config",
    "prepare:docs": "node --experimental-specifier-resolution=node node_modules/.bin/docs-shared-config",
    "prepare:husky": "husky install",
    "prepare:vscode": "node --experimental-specifier-resolution=node node_modules/.bin/vscode-shared-config",
    "release": "npm-run-all prepare lint test build generate",
    "test": "npm-run-all test:*",
    "test:jest": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@tagproject/docs-shared-config": "^1.0.4",
    "@tagproject/ts-library-shared-config": "^1.0.7",
    "@tagproject/vscode-shared-config": "^2.0.2",
    "@types/benchmark": "^2.1.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "changelog-guru": "4.0.6",
    "cspell": "^5.20.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "figma-portal": "^1.0.2",
    "ghinfo": "^3.0.6",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "mobx": "6.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.72.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "type-fest": "^3.6.1",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "mobx": "6.x"
  }
}
